{
  "name": "iocupid",
  "version": "0.1.0",
  "description": "A matchmaker for project teams.",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:webpack",
    "build:browserify": "mkdir -p build && browserify src/client/app.js -t babelify -t localenvify --outfile build/bundle.js",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.js",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .js --ext .jsx",
    "lint:fix": "eslint . --ext .js --ext .jsx --fix",
    "postinstall": "node src/postinstall.js",
    "start": "node src/index.js",
    "tape": "tape -r babel-register src/tests/**/*.js",
    "test": "npm run-script tape | tnyan",
    "test:code": "tape -r babel-register \"src/tests/**/!(eslint).js\"",
    "test:lint": "tape -r babel-register \"src/tests/**/eslint.js\""
  },
  "engines": {
    "node": "^6.2.0",
    "npm": "^3.8.9"
  },
  "keywords": [],
  "author": "Brooks Newberry",
  "license": "ISC",
  "dependencies": {
    "co": "^4.6.0",
    "common-tags": "^1.2.0",
    "css-loader": "^0.23.1",
    "dotenv-safe": "^2.3.1",
    "express": "^4.13.4",
    "flux": "^2.1.1",
    "jsonwebtoken": "^7.0.0",
    "jwt-decode": "^2.0.1",
    "lodash": "^4.13.1",
    "node-emoji": "^1.3.0",
    "node-fetch": "^1.5.2",
    "query-string": "^4.1.0",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-bootstrap-typeahead": "^0.5.1",
    "react-dom": "^15.1.0",
    "react-github-fork-ribbon": "^0.4.4",
    "react-router": "^2.4.1",
    "react-router-bootstrap": "^0.23.0",
    "react-spinkit": "^1.1.7",
    "react-tag-autocomplete": "^4.0.2",
    "rethinkdb": "^2.3.1",
    "server-destroy": "^1.0.1",
    "style-loader": "^0.13.1",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^2.0.1",
    "react": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "tap-nyan": "0.0.2",
    "tape": "^4.5.1",
    "tape-eslint": "^1.2.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-dotenv-plugin": "^1.1.0",
    "webpack-hot-middleware": "^2.10.0",
    "yakbak": "^2.4.3"
  }
}
